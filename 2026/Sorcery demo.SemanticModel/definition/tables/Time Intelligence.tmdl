table 'Time Intelligence'
	lineageTag: d63a4d27-6a17-4783-8f7f-b6fe35426820

	calculationGroup

		calculationItem AC = SELECTEDMEASURE()

		calculationItem PY = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR('Date'[FullDateAlternateKey]))

		calculationItem 'AC-PY %' =
				VAR ac_ = CALCULATE(SELECTEDMEASURE(), 'Time Intelligence'[TimeIntelligence]= "AC")
				VAR py_ = CALCULATE(SELECTEDMEASURE(), 'Time Intelligence'[TimeIntelligence]= "PY")
				
				RETURN
				DIVIDE(ac_ - py_, py_)

		calculationItem PM = CALCULATE(SELECTEDMEASURE(), PREVIOUSMONTH('Date'[FullDateAlternateKey]))

	column TimeIntelligence
		dataType: string
